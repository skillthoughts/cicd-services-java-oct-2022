pipeline
{
agent any
stages
{
stage('Checkout SCM')
{
   steps
   {
   script
   {
   git 'https://github.com/skillthoughts/cicd-services-java-oct-2022.git'
   }
   }
   }
stage('Maven Build')
{
   steps
   {
   script
   {
   sh "mvn clean package"
   }
  }
}
stage('Docker Build') {
      steps {
      script
      {
      	sh 'docker build -t skillthoughts/tomcat:1.0 .'
      }
      }
    }
}
}
